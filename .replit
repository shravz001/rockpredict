modules = ["python-3.11", "postgresql-16"]
[agent]
integrations = ["twilio_send_message:1.0.0", "python_openai:1.0.0", "python_sendgrid:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["glibcLocales", "libGL", "libGLU"]

[deployment]
deploymentTarget = "autoscale"
run = ["streamlit", "run", "app.py", "--server.port", "5000"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Rockfall Prediction System"

[[workflows.workflow]]
name = "Rockfall Prediction System"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app.py --server.port 5000"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

